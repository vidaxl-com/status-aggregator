{
  "name": "status-aggregator",
  "version": "0.0.30",
  "description": "Webservice status aggregator",
  "main": "src/index.js",
  "scripts": {
    "package-audit": "node_modules/.bin/check-audit",
    "depcheck": "node_modules/.bin/depcheck --ignores \"husky,depcheck,assert,inspector-metrics\"",
    "test": "node_modules/.bin/nyc  --reporter=lcov --reporter=clover --reporter=text --cache npm run test-dev",
    "test-dev": "node_modules/.bin/mocha test/*-spec.js",
    "test-quality": "npm run package-audit && npm run depcheck",
    "test-all": "npm run test-quality && npm test",
    "test-all-dev": "npm run test-quality && npm run test-dev"
  },
  "author": "Imre Toth",
  "repository": {
    "type": "git",
    "url": "https://github.com/vidaxl-com/status-aggregator.git"
  },
  "license": "MIT",
  "dependencies": {
    "addhttp": "^1.0.0",
    "array-flatten": "^2.1.2",
    "axios": "^0.18.0",
    "build-url": "^1.3.2",
    "camelcase": "^5.2.0",
    "dsl-framework": "^1.6.6",
    "elasticsearch": "^15.4.1",
    "is-url": "^1.2.4",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.13",
    "mysql": "^2.16.0",
    "npm": "^6.8.0",
    "random-string": "^0.2.0",
    "replaceall": "^0.1.6",
    "server-uptime": "^1.0.0",
    "time-span": "^3.0.0"
  },
  "devDependencies": {
    "array-dsl": "^1.6.14",
    "arrify": "^1.0.1",
    "assert": "^1.4.1",
    "chai": "^4.2.0",
    "cowlog": "^1.6.17",
    "debug": "^4.1.1",
    "decamelize": "^2.0.0",
    "depcheck": "^0.7.2",
    "express": "^4.16.4",
    "extract-numbers": "^1.0.2",
    "flat": "^4.1.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.2",
    "npm-audit-resolver": "^1.4.0",
    "nyc": "^13.3.0",
    "object-path": "^0.11.4",
    "portscanner": "^2.2.0",
    "require-a-lot": "^1.6.29"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-all"
    }
  },
  "files": [
    "src/**"
  ]
}
