    "install": "^0.12.2",
    "is-url": "^1.2.4",
    "npm": "^6.8.0",
    "statman-stopwatch": "^2.7.0",
    "time-span": "^2.0.0"
  },
  "devDependencies": {
    "array-dsl": "^1.6.6",
    "arrify": "^1.0.1",
    "assert": "^1.4.1",
    "chai": "^4.2.0",
    "cowlog": "^1.6.6",
    "debug": "^4.1.1",
    "depcheck": "^0.7.1",
    "express": "^4.16.4",
